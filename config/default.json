        ]
      },
      {
        "name": "events",
        "stages": [
          {
            "type": "group",
            "opts": [
              {
                "context": "#tasks",
                "elements": ".task",
                "fields": [
                  {
                    "name": "name",
                    "stages": [
                      {
                        "type": "selector",
                        "opts": [".name p"]
                      }
                    ]
                  },
                  {
                    "name": "image",
                    "stages": [
                      {
                        "type": "selector",
                        "opts": [".image img|src"]
                      }
                    ]
                  },
                  {
                    "name": "description",
                    "stages": [
                      {
                        "type": "selector",
                        "opts": [".description p"]
                      }
                    ]
                  },
                  {
                    "name": "once",
                    "stages": [
                      {
                        "type": "selector",
                        "opts": [".once"]
                      }
                    ]
                  },
                  {
                    "name": "socket",
                    "stages": [
                      {
                        "type": "selector",
                        "opts": [".socket"]
                      }
                    ]
                  },
                  {
                    "name": "file",
                    "stages": [
                      {
                        "type": "selector",
                        "opts": [".file"]
                      }
                    ]
                  },
                  {
                    "name": "notexists",
                    "stages": [
                      {
                        "type": "selector",
                        "opts": [".notexists"]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
